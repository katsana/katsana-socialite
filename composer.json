{
    "name": "katsana/socialite",
    "description": "KATSANA Socialite Provider",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\Socialite\\": "src/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/container": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "katsana/katsana-sdk-laravel": "^1.1",
        "socialiteproviders/manager": "^4.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
