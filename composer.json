{
    "name": "katsana/socialite",
    "description": "KATSANA Socialite Provider",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\Socialite\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/container": "^5.8",
        "illuminate/support": "^5.8",
        "katsana/katsana-sdk-laravel": "^1.1",
        "socialiteproviders/manager": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.9",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
